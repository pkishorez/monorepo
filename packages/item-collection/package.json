{
  "name": "item-collection",
  "dependencies": {
    "@std-toolkit/eschema": "5.0.9",
    "@std-toolkit/idb": "5.0.9",
    "effect": "3.19.13"
  },
  "devDependencies": {
    "@tanstack/react-db": "0.1.59",
    "@total-typescript/ts-reset": "0.6.1",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "25.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "type-fest": "5.3.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    }
  },
  "peerDependencies": {
    "@tanstack/react-db": ">=0.1.26"
  },
  "scripts": {
    "build": "tsc --project build.tsconfig.json",
    "dev": "tsc --watch --project build.tsconfig.json",
    "lint": "pnpm lint:tsc && pnpm lint:eslint",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:run": "vitest run --reporter=verbose",
    "test:watch": "vitest --watch --reporter=verbose"
  },
  "type": "module"
}
