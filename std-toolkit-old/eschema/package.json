{
  "name": "@std-toolkit/eschema",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*"
    }
  },
  "scripts": {
    "build": "tsc --project build.tsconfig.json",
    "dev": "tsc --watch --project build.tsconfig.json",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@standard-schema/spec": "1.1.0",
    "@tsconfig/node22": "22.0.5",
    "@valibot/to-json-schema": "1.5.0",
    "effect": "3.19.14",
    "type-fest": "5.4.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17",
    "zod": "4.3.5"
  }
}
