{
  "name": "rpc",
  "version": "0.0.1",
  "private": true,
  "description": "RPC related utilities for STD toolkit",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@effect/rpc": "0.73.0",
    "@effect/vitest": "0.27.0",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "25.0.9",
    "effect": "3.19.14",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17"
  },
  "peerDependencies": {
    "@effect/rpc": ">=0.73.0",
    "effect": ">=3.0.0"
  }
}
