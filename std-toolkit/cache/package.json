{
  "name": "@std-toolkit/cache",
  "version": "0.0.1",
  "private": true,
  "description": "Effect-based cache with IndexedDB support for EntityType storage",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./idb": {
      "types": "./dist/idb/index.d.ts",
      "default": "./dist/idb/index.js"
    },
    "./memory": {
      "types": "./dist/memory/index.d.ts",
      "default": "./dist/memory/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@std-toolkit/core": "workspace:*",
    "@std-toolkit/eschema": "workspace:*",
    "idb": "8.0.3"
  },
  "devDependencies": {
    "@effect/vitest": "0.27.0",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "25.1.0",
    "effect": "3.19.15",
    "fake-indexeddb": "6.2.5",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "effect": ">=3.0.0"
  }
}
